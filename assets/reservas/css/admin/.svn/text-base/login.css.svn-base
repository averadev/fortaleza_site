.centerDiv{
	position:relative;
	height:230px;
	left:50%;
	margin-left: -250px;
	height:230px;	
	width:839px;
}
.divLogo{
	position:relative;
	left:50%;
	margin-left: -411px;
	width:550px;
	height:200px;	
