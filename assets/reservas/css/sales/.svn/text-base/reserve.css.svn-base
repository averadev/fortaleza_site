 /** CSS Anythingslider custom rules */
 div.anythingSlider .anythingWindow{ border: none !important;}
 div.anythingSlider.activeSlider .anythingWindow{ border-color: none !important;}
 div.anythingSlider{ padding:0px !important;} 
 
 /** Tamaï¿½o de jquery.datepicker*/
 .ui-widget{ font-size:0.7em;}
 .ui-datepicker{ width:16em !important}
 
 /**Evita conflicto con blockUI*/
 .date-picker{ z-index:0 !important;}

 .SpinnerControl{ float:none; display:inline-block;} 
