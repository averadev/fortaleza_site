 }

  
 /****************************************************************************/
 /** Panel 1                                                                  */
 /****************************************************************************/
 
 #formReservation { 
 	float:left;
 	margin-top:18px;		
 }
 
 #formReservation #arriveDate,
 #formReservation #departureDate{
 	width:170px;
 	text-align:center
 }
 
 #peopleBox { 
 	margin:10px auto;
 	width:345px; 	
 }
 
 #peopleBox > select, #peopleBox > label{
 	display:inline;
 }
 
 .adultBox,  .childBox{
 	width: 45px;
 	display:inline-block;
 	margin: 0 10px 10px 5px;
 }
 /*.childBox{ width:45px;}*/
 .childAgeBox{
 	width: auto;
 	display:inline-block;
 	margin: 0 10px 10px 5px;
 }
 
 .childAge{
 	display:inline-block;
 	width:50px;
 }
 
 #calendarBox{
 	float:right;
 	width:auto; 	
 	height:auto;
 	margin-left:8px;
 }
 
 #calendarBox #modalCalendar{ text-align:center}
 
 /****************************************************************************/
 /** Panel 2                                                                  */
 /****************************************************************************/
 
 #roomConteiner{
 	width:670px;
 	overflow-x:hidden;
 	overflow-y:auto;
 	border: 1px solid #D1D2D3;
 }
 
 #reserveRange{ 
 	float:left;
 	width:320px;
 	margin:0 10px 15px 10px; 	
 	
 }
 #reserveTotal{ 
 	float:right;
 	width:300px;
 	margin:0 10px 15px 10px;
 	text-align:right;
 	font-size:1.8em;
 }
 
 div.roomTable{
 	width:591px; 	
 }
 
 table.tableRooms{ 	
 	padding: 0;
	margin: 0;
	border: 7px solid #D8D8D8;
	width:590px;
 }

 .roomSelected{border: 7px solid #6E6E6E !important;} 
 
 table.tableRooms caption {
	padding: 0 0 5px 0;
	width: 700px;	 
	font: italic 11px "Trebuchet MS", Verdana, Arial, Helvetica, sans-serif;
	text-align: right;
}

 table.tableRooms th {
	font: bold 11px "Trebuchet MS", Verdana, Arial, Helvetica, sans-serif;
	color: #FFFFFF;
	border-right: 1px solid #C1DAD7;
	border-bottom: 1px solid #C1DAD7;
	border-top: 1px solid #C1DAD7;
	letter-spacing: 0px;
	text-transform: uppercase;
	text-align: left;
	padding: 6px;
	background: #A9ABB5;
}

 table.tableRooms td.alt {
	background: #FFFFFF;
	color: #797268;
	text-align:center;
}
 
 .roomTypeName{	
 	font-weight:bold;
 	font-size: 18px;
 	letter-spacing:1px; 	
 } 
 
 .roomItem{	margin:0 10px 25px 10px;}
  
 .numRooms{	text-align:center; }
 
 .colSelect { text-align:center;}
 
 .colRooms{ text-align:center;	height:38px;}
 
 .prevPrice{text-decoration:line-through;}
 
 .tRoomPrice{
 	font-size:18px;
 	font-weight:bold;
 	float:right;
 	letter-spacing:2px; 	
 } 
 
 .xtraPerson{
 	font-size:11px;
 	font-weight:bold;
 	color: #B40431;
 	float:right;
 	margin-right: 25px;
 	text-align: right;
 } 
 
 .notAvailable{
 	font-size:12px;
 	font-weight:bold;
 	float:right;
 	letter-spacing:2px;
 	color: red; 	
 }
 /****************************************************************************/
 /** Panel 3                                                                 */
 /****************************************************************************/
 #customerInfo{ margin: 10px 0 0 12px;}
 #cFormTitle {
 	font-weight:bold;
 	font-size:24px;
 	text-align:center;
 	margin-bottom:30px;
 }
 #formCustomer input, #formCustomer select { width: 185px;}
 
 /****************************************************************************/
 /** Panel 4                                                                  */
 /****************************************************************************/
 #reserveSummary{ 
 	displaY:inline-block;
 	width:660px;
 	height:350px;
 }
 
 #generalData{
 	float:left;
 	margin: 10px 15px 0 0; 
 } 
  
 #generalPrice{ float: left;}
 
 #formPaypal {
 	width:250px;
 	height:166px; 	
 	cursor:pointer;
 	float:right;
 	margin-top:55px; 
 }
 
 #formPaypal #btnPay{
 	width:195px;
 	height:135px;
 	margin-top:-25px;
 }
 #legendPaypal{
 	width:195px;
 	color:#880000;
 	font-size: small;
 }
 .childAgeBox{
 	margin-top:10px;
 	font-size: smaller;
 	color:#444444;
 }
 
 .titleArrive, .titleDeparture{
 	display:inline-block; 	
 	width:65px;
 	font-size:1.2em;
 }
 .txtArrive, .txtDeparture{ 	
 	width:100px;
 	font-size:1.2em;
 }
 
 #reservePrice,#reserveCode{
 	font-size:2em;
 }  
 #roomSummary{
 	width:675px;
 	height:300px;
 	clear:both;
 	margin-top:0;
 	overflow-x:hidden;
 	overflow-y:auto;
 }
 .roomForPay{
 	margin-bottom:10px;
 }
 
 .sumRoom  { float: left;}
 .sumPrice { float: right;}
 .sumDesc{ clear:both; width:590px;}
 
 #newReserve{
 	clear:both;
 	margin-top:320px;
 	text-algin:center;
 	cursor:pointer;
 	font-size:18px; 	 	
 }
 
 #totalData{
 	display:inline-block;
 	margin:10px 0 0 15px;
 }

